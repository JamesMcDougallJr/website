{
    "name": "mywebsite",
    "private": true,
    "version": "0.0.1",
    "description": "Stencil App Starter",
    "homepage": "https://github.com/JamesMcDougallJr/james.git",
    "scripts": {
        "build": "stencil build",
        "start": "stencil build --dev --watch --serve",
